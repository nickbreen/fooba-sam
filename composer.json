{
  "name": "foobar/wp",
  "description": "WordPress stack via Composer",
  "authors": [
    {
      "name": "Nick Breen",
      "homepage": "https://github.com/nickbreen/"
    }
  ],
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "config": {
    "vendor-dir": "wp-content/vendor",
    "sort-packages": true
  },
  "archive": {
    "exclude": [
      "*" ,
      "!index.php",
      "!wp-config.php",
      "!wp/",
      "wp/wp-content/",
      "!wp-content/"
    ]
  },
  "require": {
    "php": ">=7.3.0",
    "johnpbloch/wordpress": "5.1.1"
  },
  "require-dev": {
    "wpackagist-theme/twentyten": "2.8",
    "wpackagist-theme/twentyeleven": "3.2",
    "wpackagist-theme/twentytwelve": "2.9",
    "wpackagist-theme/twentythirteen": "2.8",
    "wpackagist-theme/twentyfourteen": "2.6",
    "wpackagist-theme/twentyfifteen": "2.4",
    "wpackagist-theme/twentysixteen": "1.9",
    "wpackagist-theme/twentyseventeen": "2.1",
    "wpackagist-theme/twentynineteen": "1.3"
  },
  "extra": {
    "wordpress-install-dir": "wp"
  }
}
