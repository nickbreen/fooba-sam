{
  "name": "foobar/wp",
  "description": "WordPress stack via Composer",
  "authors": [
    {
      "name": "Nick Breen",
      "homepage": "https://github.com/nickbreen/"
    }
  ],
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "config": {
    "use-include-path": true,
    "vendor-dir": "wp-content/vendor",
    "sort-packages": true,
    "platform": {
      "php": "7.3.6",
      "ext-bcmath": "7.3.6",
      "ext-curl": "7.3.6",
      "ext-exif": "7.3.6",
      "ext-filter": "7.3.6",
      "ext-fileinfo": "7.3.6",
      "ext-mbstring": "7.3.6",
      "ext-mysqli": "7.3.6",
      "ext-libsodium": "7.3.6",
      "ext-openssl": "7.3.6",
      "ext-pcre": "7.3.6",
      "ext-imagick": "7.3.6",
      "ext-xml": "7.3.6",

      "ext-gd": "7.3.6",
      "ext-mcrypt": "7.3.6",
      "ext-xmlreader": "7.3.6",
      "ext-zlib": "7.3.6",

      "ext-json": "7.3.6",
      "ext-pcntl": "7.3.6"
    }
  },
  "archive": {
    "exclude": [
      "*" ,
      "!index.php",
      "!wp-config.php",
      "!wp/",
      "wp/wp-content/",
      "!wp-content/"
    ]
  },
  "prefer-stable": true,
  "require": {
    "johnpbloch/wordpress": "^5.1.1",
    "wpackagist-theme/twentyten": "*",
    "wpackagist-theme/twentyeleven": "*",
    "wpackagist-theme/twentytwelve": "*",
    "wpackagist-theme/twentythirteen": "*",
    "wpackagist-theme/twentyfourteen": "*",
    "wpackagist-theme/twentyfifteen": "*",
    "wpackagist-theme/twentysixteen": "*",
    "wpackagist-theme/twentyseventeen": "*",
    "wpackagist-theme/twentynineteen": "*",
    "wpackagist-plugin/hello-dolly": "*"
  },
  "extra": {
    "wordpress-install-dir": "wp"
  }
}
